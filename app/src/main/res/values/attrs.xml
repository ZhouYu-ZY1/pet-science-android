<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--圆形进度条配置-->
    <declare-styleable name="RingProgressBar">
        <attr name="rRingColor" format="color" />
        <attr name="rTextColor" format="color" />
        <attr name="rTextSize" format="dimension" />
        <attr name="rCircleColor" format="color" />
        <attr name="rProgressColor" format="color" />
        <attr name="rMax" format="integer" />
        <attr name="progress" format="integer"/>
        <attr name="outerRingWidth" format="dimension"/>
        <attr name="rTextIsVisibility" format="boolean" />
        <attr name="style">
            <enum name="STROKE" value="0" />
            <enum name="FILL" value="1" />
        </attr>
    </declare-styleable>

    <!--自动换行linearLayout-->
    <declare-styleable name="WarpLinearLayout">
        <attr name="grivate" format="enum"><!--对齐方式!-->
            <enum name="right" value="0" />
            <enum name="left" value="1" />
            <enum name="center" value="2" />
        </attr>
        <attr name="horizontal_Space" format="dimension" />
        <attr name="vertical_Space" format="dimension" />
        <attr name="isFull" format="boolean" />
    </declare-styleable>
</resources>
